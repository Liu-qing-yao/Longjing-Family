.arrow-trans image {
            -webkit-animation: myfirst 0.9s infinite;
                    animation: myfirst 0.9s infinite;
}
@-webkit-keyframes myfirst {
0% {
                -webkit-transform: translate(0rpx, 0rpx);
                        transform: translate(0rpx, 0rpx);
}
50% {
                -webkit-transform: translate(-12rpx, 0rpx);
                        transform: translate(-12rpx, 0rpx);
}
100% {
                -webkit-transform: translate(0rpx, 0rpx);
                        transform: translate(0rpx, 0rpx);
}
}
@keyframes myfirst {
0% {
                -webkit-transform: translate(0rpx, 0rpx);
                        transform: translate(0rpx, 0rpx);
}
50% {
                -webkit-transform: translate(-12rpx, 0rpx);
                        transform: translate(-12rpx, 0rpx);
}
100% {
                -webkit-transform: translate(0rpx, 0rpx);
                        transform: translate(0rpx, 0rpx);
}
}
